{
  "name": "netlify-og-image",
  "scripts": {
    "serve:template": "parcel serve ./src/template.html",
    "build:template": "parcel build ./src/template.html --dist-dir ./netlify/functions/og-image/",
    "build:function": "cp ./src/og-image.ts ./netlify/functions/og-image/index.ts",
    "clean": "rm -rf ./netlify",
    "build": "yarn clean; yarn build:template; yarn build:function;",
    "lint": "eslint --fix src/*.js; prettier --write .;"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.2.1",
    "eslint": "^8.9.0",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@netlify/functions": "^1.4.0",
    "chrome-aws-lambda": "~7.0.0",
    "normalize.css": "^8.0.1",
    "puppeteer-core": "~7.0.0"
  }
}
